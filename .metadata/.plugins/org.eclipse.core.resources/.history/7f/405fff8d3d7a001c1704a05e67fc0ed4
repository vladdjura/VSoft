package javaaaa;

public class Questions {

	public Questions() {
		// TODO Auto-generated constructor stub
	}
	
	public void swapTwoNumbers(int a, int b) {
		int temp = a;
		a = b;
		b = temp;
		
		System.out.println("after swapping");
		System.out.println("value a ="+ a);
		System.out.println("value b ="+ b);
	}
	
}
